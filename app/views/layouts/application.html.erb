<!DOCTYPE html>
<html>
<head>
  <title>Rto</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if user_signed_in? %>
	Welcome <%= current_user.username %> 
	<%= link_to "Account Setting", edit_user_registration_path %>
	<%= link_to "Logout", destroy_user_session_path, method: :delete %> <br/><br/>

 <% if current_user.is_admin %>
	<%= link_to "states", states_path %> | <%= link_to "Registration Numbers",
	 registration_numbers_path %> | <%= link_to "Localities", localities_path %>
	 <% end %>
<% else %>
	<%= link_to "Login", new_user_session_path %>
	<%= link_to "Register", new_user_registration_path %>
<% end %>


<%= alert %>

<%= yield %>

</body>
</html>
